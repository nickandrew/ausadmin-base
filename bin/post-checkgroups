#!/bin/bash
#	@(#) bin/post-checkgroups - Post signed checkgroups
#
# $Source$
# $Revision$
# $Date$

umask 022

#----------------------------------------------------------------------------
#  Post the message
#----------------------------------------------------------------------------

rc=0

bin/post.pl < data/checkgroups.msg
s=$?
if [ $s -ne 0 ] ; then
	echo "post.pl submission of data/checkgroups.msg returned status $s"
	rc=8
fi

exit $rc
